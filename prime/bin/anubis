#!/usr/bin/env python3
# EASY-INSTALL-ENTRY-SCRIPT: 'anubis-netsec==0.8.3','console_scripts','anubis'
__requires__ = 'anubis-netsec==0.8.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('anubis-netsec==0.8.3', 'console_scripts', 'anubis')()
    )
